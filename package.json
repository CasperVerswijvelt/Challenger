{
  "name": "challengerapp",
  "version": "1.0.0",
  "description": "Challenger app",
  "homepage": "http://www.github.com/pieter-hogent/",
  "scripts": {
    "start": "node server",
    "deploy-backend": " git push origin 'git subtree split --prefix backend challenger-backend master' --force",
    "build-frontend": "cd frontend && ng build --prod",
    "commit-build-changes": "git add . && git commit -m build_files",
    "deploy-frontend": "npm run build-frontend && npm run copy && npm run commit-build-changes && git subtree push --prefix serve-frontend challenger-frontend master -f",
    "copy": "cpx 'frontend/dist/**' serve-frontend/www"
  },
